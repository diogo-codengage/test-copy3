{
    "name": "sanar-ui",
    "version": "0.1.0",
    "license": "MIT",
    "main": "dist/bundle.js",
    "style": "dist/bundle.less",
    "scripts": {
        "build": "cross-env BABEL_ENV=build babel src -d dist --source-maps --delete-dir-on-start --config-file ./babel.config.js --no-comments --copy-files",
