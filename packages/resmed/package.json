        "typescript": "^3.5.3"
    },
    "scripts": {
        "release-major": "yarn version --major --no-git-tag-version",
        "release-minor": "yarn version --minor --no-git-tag-version",
        "release-patch": "yarn version --patch --no-git-tag-version",
        "start": "env-cmd -f ./environments/.env.development react-app-rewired start",
        "build:development": "env-cmd -f ./environments/.env.development react-app-rewired build",
        "build:staging": "env-cmd -f ./environments/.env.development react-app-rewired build",
        "build:production": "env-cmd -f ./environments/.env.production react-app-rewired build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "ie 11",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/react-router-dom": "^4.3.4",
        "@types/styled-components": "^4.1.18"
    }
